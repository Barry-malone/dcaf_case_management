<div id="overview" class="margin-bottom">
  <h1 class="border-bottom title">Week of <%= week_range %></h1>

  <div id="budget_bar">
    <%#= render_async budget_bar_path, nonce: content_security_policy_script_nonce, 'data-turbolinks-track': 'reload' %>
    <div class="progress">
      <%# expenditures.each_pair do |type, value| %>
        <div class="progress-bar progress-bar-striped <%#= progress_bar_color type %>" style="<%#= progress_bar_width value %>">$<%#= value %></div>
      <%# end %>
    </div>
  </div>
  

</div>
